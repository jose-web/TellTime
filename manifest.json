{
    "manifest_version": 2,
    "name": "TellTime",
    "description": "This extension tells the time in a robot voice.",
    "version": "1.0",
    "icons": {
        "128": "128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "background"
    ]
}