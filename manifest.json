{
    "manifest_version": 2,
    "name": "tellTime",
    "description": "Te dice la hora",
    "version": "1.0",
    "icons": {
        "128": "128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "<all_urls>",
        "activeTab"
    ]
}